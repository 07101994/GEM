    <activity
        android:name=".ui.setup.SetupActivity"
        android:label="@string/app_name_actual" />
    <activity
        android:name=".ui.settings.Settings"
        android:label="@string/title_activity_settings" />
    <activity
        android:name=".ui.settings.chooseIcon.ChooseIcon"
        android:label="@string/title_activity_choose_icon" />

    <receiver android:name=".ui.MusicPlaybackWidget" >
        <intent-filter>
            <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
        </intent-filter>

        <meta-data
            android:name="android.appwidget.provider"
            android:resource="@xml/music_playback_widget_info" />
    </receiver>

    <service android:name=".media.MediaService">
        <intent-filter>
            <action android:name="android.intent.action.MEDIA_BUTTON" />
        </intent-filter>
    </service>

    <receiver android:name="android.support.v4.media.session.MediaButtonReceiver">
        <intent-filter>
            <action android:name="android.intent.action.MEDIA_BUTTON" />
        </intent-filter>
    </receiver>

    <activity
        android:name=".ui.settings.AlbumColor"
        android:label="@string/title_activity_album_color" />
    <activity
        android:name=".ui.Search"
        android:label="@string/title_activity_search"
        android:parentActivityName=".ui.mainScreen.MainScreen" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.animbus.music.ui.mainScreen.MainScreen" />
    </activity>
    <activity
        android:name=".UiTweaker"
        android:label="@string/title_activity_ui_customiser" />
    <activity
        android:name=".ui.albumDetails.AlbumDetails"
        android:label="@string/title_activity_album_details"
        android:theme="@style/AppTheme.Light" />
    <activity
        android:name=".ui.nowPlaying.NowPlaying"
        android:label="@string/title_activity_now_playing"
        android:theme="@style/AppTheme.Light"
        android:launchMode="singleTop" />

    <!-- Media button receiver -->


</application>

</manifest>
