<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.animbus.music" >

<uses-permission android:name="android.permission.WAKE_LOCK" />
<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
<uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
<uses-permission android:name="com.android.vending.BILLING" />

<!-- <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT"/> -->
<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher_srini_black"
    android:label="@string/app_name_actual"
    android:theme="@style/AppTheme" >
    <activity
        android:name=".ui.mainScreen.MainScreen"
        android:label="@string/app_name" />
    <activity android:name=".ui.LaunchActivity"
        android:label="@string/app_name" >

        <intent-filter>
            <action android:name="android.intent.action.VIEW" />
            <category android:name="android.intent.category.APP_MUSIC" />
            <category android:name="android.intent.category.DEFAULT" />
            <data android:mimeType="audio/*" />
        </intent-filter>
    </activity>

    <activity-alias
        android:name=".activities.app.icon.srini.black"
        android:enabled="true"
        android:icon="@mipmap/ic_launcher_srini_black"
        android:label="@string/app_name"
        android:targetActivity=".ui.LaunchActivity" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity-alias>
    <activity-alias
        android:name=".activities.app.icon.srini.white"
        android:enabled="false"
        android:icon="@mipmap/ic_launcher_srini_white"
        android:label="@string/app_name"
        android:targetActivity=".ui.LaunchActivity" >
